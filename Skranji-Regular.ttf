<script>

		$(function () {
			var img = new Image();
			img.onload = function () { UpdateScreen(); }
			img.src = "images/roadmap/frame.png";
		});
		$(window).resize(function () {
			UpdateScreen();
		});

		function UpdateScreen() {
			var height = $("#roadmap .card:eq(0)").height();
			var ratio = height / 409;
			$("#roadmap .line-roadmap").height(height * 5 + 30 * 5);
			$("#roadmap p").css({ fontSize: ratio * 20 });
			$(".paragraph p").css({ fontSize: $(".paragraph ").outerHeight() / 354 * 22 });
			$(".title-head").css({ fontSize: $(".title-head:eq(0)").parent().height() / 131 * 40 });
		}

		new spine.SpinePlayer("ani-heo", {
			jsonUrl: "Animation/Ani/AniHeo/Heo_front.json",
			atlasUrl: "Animation/Ani/AniHeo/Heo_front.atlas.txt",
			animation: "Idle",
			alpha: true,
			showControls: false
		});
		new spine.SpinePlayer("ani-khunglong", {
			jsonUrl: "Animation/Ani/AniKhungLong/KhungLong_back.json",
			atlasUrl: "Animation/Ani/AniKhungLong/KhungLong_back.atlas.txt",
			animation: "Idle",
			alpha: true,
			showControls: false,
			viewport: {
				animations: {
					"Idle": {
						x: -150,
						y: 0,
						padLeft: "0%",
						padRight: "0%",
						padTop: "0%",
						padBottom: "0%"
					}
				}
			}
		});
		new spine.SpinePlayer("ani-bocap", {
			jsonUrl: "Animation/Ani/AniBoCap/BoCap_front 2.json",
			atlasUrl: "Animation/Ani/AniBoCap/BoCap_front 2.atlas.txt",
			animation: "Idle",
			alpha: true,
			showControls: false,
			viewport: {
				animations: {
					"Idle": {
						x: -150,
						y: 0,
						width: 300,
						height: 500,
						padLeft: "0%",
						padRight: "0%",
						padTop: "0%",
						padBottom: "0%"
					}
				}
			}
		});

		new spine.SpinePlayer("ani1", {
			jsonUrl: "monsters/PIG.json",
			atlasUrl: "monsters/PIG.atlas",
			animation: "idle",
			alpha: true,
			showControls: false
		});
		new spine.SpinePlayer("ani2", {
			jsonUrl: "monsters/KL.json",
			atlasUrl: "monsters/KL.atlas",
			animation: "idle",
			alpha: true,
			showControls: false
		});
		new spine.SpinePlayer("ani3", {
			jsonUrl: "monsters/BC.json",
			atlasUrl: "monsters/BC.atlas",
			animation: "idel",
			alpha: true,
			showControls: false
		});
	</script>
<script>
		document.addEventListener('scroll', () => {
			let dom = document.querySelectorAll(".line-roadmap .card");
			const width = window.innerWidth;
			const postY = window.pageYOffset;
			console.log(postY);
			console.log(width < 770);
			if (width < 770) {
				if (width > 300) {
					if (postY > 6100 && postY < 7600) {

						dom.forEach((e, index) => {
							setTimeout(() => {

								e.classList.add('mobile_map')
							}, index * 1000)
						})
						// dom.classList.add('mobile_map')
					} else {

					}
				} else {
					if (postY > 6100) {
						dom.classList.add('mobile_map')
					}
				}
			} else {
				if (postY > 6100) {
					dom.classList.add('desktop_map')
				}

			}

		});

	</script>
